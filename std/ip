ping.all(){
    for ip in 192.168.{1..1}.{1..255}; do
        ping $ip -c 2 > /dev/null
        if [ $? -eq 0 ]; then
            echo $ip is alive
        fi
    done
}

# Not good at it
ip.ping(){
    while read -d ' ' ip; do
        (
            ping $ip -c 2 &> /dev/null
            if [ $? -eq 0 ]; then
                echo $ip is alive
            fi
        ) &
    done
}



