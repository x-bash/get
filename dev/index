dev.xbash.push(){
    git remote remove gitee 2>/dev/null
    git remote add gitee git@gitee.com:x-bash/x-bash.git
    git push gitee

    git remote remove github 2>/dev/null
    git remote add github git@github.com:x-bash/x-bash.github.io.git
    git push github

    git remote remove gitee-mirror 2>/dev/null
    git remote add gitee-mirror git@gitee.com:x-bash/x-bash-boot-mirror.git
    git push gitee-mirror

    git remote remove github-mirror 2>/dev/null
    git remote add github-mirror git@github.com:x-bash/x-bash-boot-mirror.git
    git push github-mirror

    git push origin
}

dev.xbash.publish(){
    xrc std/http
    http.browse https://gitee.com/x-bash/x-bash/pages
}

dev.xbash.clear(){
    rm !(el)
    rm !(dev)
    rm !(.vscode)

    for i in */; do
        case "$i" in
        el/)        continue;;
        dev/)       continue;;
        .vscode/)   continue;;
        */)         rm -rf "$i";;
        esac
    done
}
